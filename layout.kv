<LoginScreen>:
	name: "Login_Screen"
	id: Login
	MDFloatLayout
		Image: 
			source: "assets/blue_bg.png"
			size_hint: 1, 1
			pos_hint: {'center_x': 0.5, 'center_y': 0.5}
		Image:
			source: "assets/App_logo.png"
			size_hint: 0.4, 0.4
			pos_hint: {'center_x': 0.5, 'center_y': 0.8}
		Label:
			text: "LOGIN"
			pos_hint: {'center_x': 0.5, 'center_y': 0.65}
			halign: 'center'
			color: 0/255, 0/255, 0/255
			font_size: "27sp"
			font_name: "LatoB"

		MDTextField:
			hint_text: "Username/Email"
			font_name: "OpenSansR"
			font_size: "13dp"
			icon_right: "account"
			height: 100
			size_hint: None , None
			line_color_normal: (0, 0, 0, 1)
			width: 200
			pos_hint: {"center_x": .5, "center_y": .55}
			helper_text:  ""

		MDTextField:
			id: password
			hint_text: "Password"
			font_name: "OpenSansR"
			font_size: "13dp"
			icon_right: "account"
			height: 100
			size_hint: None , None
			line_color_normal: (0, 0, 0, 1)
			width: 200
			pos_hint: {"center_x": .5, "center_y": .45}
			helper_text:  ""
			password: True

		MDCheckbox:
			size_hint: None, None
			size: "20dp", "20dp"
			pos_hint: {"center_x": .73, "center_y": .40}
			on_active:
				root.show_password(*args)

		MDLabel:
			id: pass_text
			text: "Show Password"
			font_size: "10sp"
			pos_hint: {"center_x": .99, "center_y": .40}


		MDFlatButton:
			text: "            Continue with Google"
			font_size: "13sp"
			font_name: "LatoB"
			padding: [20, 3]
			mode: "rectangle"
			pos_hint: {'center_x': .5, 'center_y': .32}
			md_bg_color: 235/255, 255/255, 255/255, 1

			MDFloatLayout:
				Image:
					source: "assets/Google.ico"
					size_hint: .85, .85
					pos_hint: {'center_x': 0.02, 'center_y': 0.5}


		MDFlatButton:
			text: "          Continue with Facebook"
			font_size: "13sp"
			font_name: "LatoB"
			theme_text_color: "Custom"
			text_color: 255/255, 255/255, 255/255
			padding: [16, 3]
			mode: "rectangle"
			pos_hint: {'center_x': .5, 'center_y': .25}
			md_bg_color: 66/255, 103/255, 178/255, 1

			MDFloatLayout:
				Image:
					source: "assets/Facebook.ico"
					size_hint: .85, .85
					pos_hint: {'center_x': 0.03, 'center_y': 0.5}

		MDRaisedButton:
    		text: "Log In"
    		font_size: '15dp'
    		text_color: 0/255, 0/255, 0/255
    		padding: [90, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .5, 'center_y': .14}
    		md_bg_color: 235/255, 233/255, 187/255, 1

    	MDRaisedButton:
    		text: "Create New Account"
    		font_size: '15dp'
    		text_color: 0/255, 0/255, 0/255
    		padding: [39.3, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .5, 'center_y': .08}
    		md_bg_color: 255/255, 222/255, 89/255, 1
    		on_release:
    			root.manager.transition.direction = "left"
    			root.manager.current = "Register_Screen"

<RegisterScreen>:
	name: "Register_Screen"
	id: Register
	MDFloatLayout
		Image: 
			source: "assets/blue_bg.png"
			size_hint: 1, 1
			pos_hint: {'center_x': 0.5, 'center_y': 0.5}
		Image:
			source: "assets/App_logo.png"
			size_hint: 0.3, 0.3
			pos_hint: {'center_x': 0.5, 'center_y': 0.85}

		MDIconButton:
			icon: "arrow-left"
			pos_hint: {"center_x": .1 ,"center_y": .95}
			user_font_size: "30sp"
			on_release:
				root.manager.transition.direction = "right"
				root.manager.current = "Login_Screen"

		Label:
			text: "REGISTER"
			pos_hint: {'center_x': 0.5, 'center_y': 0.74}
			halign: 'center'
			color: 0/255, 0/255, 0/255
			font_size: "22sp"
			font_name: "LatoB"

		MDTextField:
			id: Fname
			hint_text: "First Name"
			font_name: "OpenSansR"
			font_size: "13dp"
			size_hint_x: None 
			line_color_normal: (0, 0, 0, 1)
			helper_text_mode: "persistent"
			width: 200
			pos_hint: {"center_x": .5, "center_y": .67}
			helper_text:  ""

		MDTextField:
			id: Lname
			hint_text: "Last Name"
			font_name: "OpenSansR"
			font_size: "13dp"
			size_hint_x: None 
			line_color_normal: (0, 0, 0, 1)
			helper_text_mode: "persistent"
			width: 200
			pos_hint: {"center_x": .5, "center_y": .59}
			helper_text:  ""

		MDTextField:
			id: Uname
			hint_text: "Username"
			font_name: "OpenSansR"
			font_size: "13dp"
			size_hint_x: None 
			line_color_normal: (0, 0, 0, 1)
			helper_text_mode: "persistent"
			width: 200
			pos_hint: {"center_x": .5, "center_y": .51}
			helper_text:  ""

		MDTextField:
			id: Reg_Email
			hint_text: "Email"
			font_name: "OpenSansR"
			font_size: "13dp"
			helper_text_mode: "persistent"
			size_hint_x: None
			line_color_normal: (0, 0, 0, 1)
			width: 200
			pos_hint: {"center_x": .5, "center_y": .43}
			helper_text:  ""

		MDTextField:
			id: Rpassword
			hint_text: "Password"
			font_name: "OpenSansR"
			font_size: "13dp"
			size_hint_x: None 
			helper_text_mode: "persistent"
			line_color_normal: (0, 0, 0, 1)
			width: 200
			pos_hint: {"center_x": .5, "center_y": .34}
			helper_text:  ""
			password: True

		MDCheckbox:
			size_hint: None, None
			size: "20dp", "20dp"
			pos_hint: {"center_x": .73, "center_y": .28}
			on_active:
				root.show_Rpassword(*args)

		MDLabel:
			id: pass_text
			text: "Show Password"
			font_size: "10sp"
			pos_hint: {"center_x": .99, "center_y": .275}

		MDTextField:
			id: RCpassword
			hint_text: "Confirm Password"
			font_name: "OpenSansR"
			font_size: "13dp"
			size_hint_x: None 
			line_color_normal: (0, 0, 0, 1)
			width: 200
			helper_text_mode: "persistent"
			pos_hint: {"center_x": .5, "center_y": .225}
			helper_text:  ""
			password: True

		MDCheckbox:
			size_hint: None, None
			size: "20dp", "20dp"
			pos_hint: {"center_x": .73, "center_y": .165}
			on_active:
				root.show_RCpassword(*args)

		MDLabel:
			id: pass_text
			text: "Show Password"
			font_size: "10sp"
			pos_hint: {"center_x": .99, "center_y": .160}

		MDRaisedButton:
    		text: "Register"
    		font_size: '15dp'
    		text_color: 0/255, 0/255, 0/255
    		padding: [70, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .5, 'center_y': .09}
    		md_bg_color: 255/255, 222/255, 89/255, 1
    		on_release:
    			root.regverification(Fname.text, Lname.text, Uname.text, Reg_Email.text, Rpassword.text, RCpassword.text)

