<LoginPage>:
	name: "Login_Page"
	id: loginpage
	MDFloatLayout:
		Image:
			source: "assets/secondary_bg.png"
			size_hint: 1, 1
			pos_hint: {'center_x': 0.5, 'center_y': 0.7}

	    Image:
			source: "assets/primary_logo.png"
			size_hint: 0.6, 0.6
			pos_hint: {'center_x': 0.5, 'center_y': 0.75}

	    MDLabel:
			text: "LOGIN"
			pos_hint: {'center_x': 0.5, 'center_y': 0.7}
			halign: 'center'
			color: 0, 0, 0
			font_size: "10sp"
			font_name: "Montserrat"
			bold: True

	    MdTextFieldRound: 
			hint_text: "Username/Email"
			icon_right: "account"
			normal_color: app.theme_cls.accent_color
        	color_active: 204, 204, 204, 1
			size_hint_x: None 	
			width: 200
			font size: 20 
			pos_hint: {"center_x":.5, "center_y": .4}

	    MdTextFieldRound: 
			hint_text: "Password"
			icon_right: "eye-off"
			normal_color: app.theme_cls.accent_color
        	color_active: 204, 204, 204, 1
			size_hint_x: None 	
			width: 200
			font size: 20 
			pos_hint: {"center_x":.5, "center_y": .35 }

		Image: 
			source: "assets/fb.png"
			size_hint: 0.3, 0.3
			pos_hint:{"center_x":.4, "center_y": .3}

		Image: 
			source: "assets/google.png"
			size_hint: 0.3, 0.3
			pos_hint:{"center_x":.6, "center_y": .3}

		MDRaisedButton:
    		text: "Log In"
    		font_size: '15dp'
    		padding: [15, 3]
    		font_name: "Montserrat"
    		pos_hint: {'center_x': .5, 'center_y': .25}
    		md_bg_color: 255/255, 255/255, 128/255, 1

    	MDRaisedButton:
    		text: "Create New Account"
    		font_size: '15dp'
    		padding: [15, 3]
    		font_name: "Montserrat"
    		pos_hint: {'center_x': .5, 'center_y': .20}
    		md_bg_color: 255/255, 255/255, 128/255, 1

