<LogoScreen>:
	name: "Logo_Page"
	id: logopage
	MDFloatLayout:
		Image: 
			source: "assets/app_bg.png"
			size_hint: 1, 1
			pos_hint: {'center_x': 0.5, 'center_y': 0.7}

		Image:
			source: "assets/primary_logo.png"
			size_hint: 0.7, 0.7
			pos_hint: {'center_x': 0.5, 'center_y': 0.65}

		MDLabel:
			text: "@YourExpense"
			pos_hint: {'center_x': 0.5, 'center_y': 0.55}
			halign: 'center'
			color: 255, 204, 0
			font_size: "20sp"
			font_name: "Montserrat"
			bold: True

		MDLabel:
			text: "We keep you from \ngoing broke."
			pos_hint: {'center_x': 0.5, 'center_y': 0.45}
			halign: 'center'
			color: 255, 255, 255
			font_size: "15sp"
			font_name: "Open Sans"
			bold: True

		MDLabel:
			text: "In partnership with Banco De Oro"
			pos_hint: {'center_x': 0.5, 'center_y': 0.1}
			halign: 'center'
			color: 255, 255, 255
			font_size: "5sp"
			font_name: "Roboto"
			

