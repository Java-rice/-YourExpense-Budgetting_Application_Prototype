				if self.focus: root.Tmenu.open()

		MDTextField:
			id: InputAmount
			hint_text: "Input Amount"
			font_name: "OpenSansR"
			font_size: "15dp"
			height: 100
			helper_text_mode: "persistent"
			size_hint: None , None
			line_color_normal: (0, 0, 0, 1)
			width: 200
			pos_hint: {"center_x": .5, "center_y": .34}
			helper_text:  ""

		MDLabel:
			id: date
			text: "Date of Transaction"
			halign: 'center'
			pos_hint: {'center_x': 0.5, 'center_y': 0.25}

		MDRaisedButton:
			text: "Choose Date"
			font_size: '15dp'
    		text_color: 0, 0, 26
    		padding: [39.3, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .5, 'center_y': .20}
    		md_bg_color: 255/255, 222/255, 89/255, 1
    		on_release:
    			root.show_date()

    	MDRaisedButton:
			text: "Confirm"
			font_size: '15dp'
    		text_color: 0, 0, 26
    		padding: [39.3, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .25, 'center_y': .10}
    		md_bg_color: 255/255, 222/255, 89/255, 1
    		on_release:
    			root.getTransInfo(SelectCategories.text, SelectStatus.text, SelectType.text, InputAmount.text, date.text)

    	MDRaisedButton:
			text: "Cancel"
			font_size: '15dp'
    		text_color: 0, 0, 26
    		padding: [39.3, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .75, 'center_y': .10}
    		md_bg_color: 254/255, 77/255, 77/255, 1
    		on_release:
    			root.manager.current = "Home_Screen"



<SwipeToDeleteItem>:
    size_hint_y: None
    height: content.height

    MDCardSwipeLayerBox:
    	radius: 0
    	MDIconButton:
    		icon: "trash-can"
    		pos_hint: {"center_y":.5}
    		on_release: app.remove_item(root)
        # Content under the card.

    MDCardSwipeFrontBox:
    	radius: 0
        # Content of card.
        OneLineListItem:
            id: content
            text: root.text
            _no_ripple_effect: True

