<CurrencyPage>:
	name: "Currency_Page"
	id: currencypage
	MDFloatLayout:
		Image:
			source: "assets/secondary_bg.png"
			size_hint: 1, 1
			pos_hint: {'center_x': 0.5, 'center_y': 0.7}
		
		Image:
			source: "assets/primary_logo.png"
			size_hint: 0.3, 0.3
