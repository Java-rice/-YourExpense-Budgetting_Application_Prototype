<LoginScreen>:
	name: "Login_Screen"
	id: Login
	MDFloatLayout
		Image: 
			source: "assets/blue_bg.png"
			size_hint: 1, 1
			pos_hint: {'center_x': 0.5, 'center_y': 0.5}
		Image:
			source: "assets/App_logo.png"
			size_hint: 0.4, 0.4
			pos_hint: {'center_x': 0.5, 'center_y': 0.8}
		Label:
			text: "LOGIN"
			pos_hint: {'center_x': 0.5, 'center_y': 0.65}
			halign: 'center'
			color: 0/255, 0/255, 0/255
			font_size: "27sp"
			font_name: "LatoB"

		MDTextField:
			id: Lusername
			hint_text: "Username/Email"
			font_name: "OpenSansR"
			font_size: "13dp"
			icon_right: "account"
			helper_text_mode: "persistent"
			height: 100
			size_hint: None , None
			line_color_normal: (0, 0, 0, 1)
			width: 200
			pos_hint: {"center_x": .5, "center_y": .55}
			helper_text:  ""

		MDTextField:
			id: Lpassword
			hint_text: "Password"
			font_name: "OpenSansR"
			font_size: "13dp"
			icon_right: "account"
			helper_text_mode: "persistent"
			height: 100
			size_hint: None , None
			line_color_normal: (0, 0, 0, 1)
			width: 200
			pos_hint: {"center_x": .5, "center_y": .45}
			helper_text:  ""
			password: True

		MDCheckbox:
			size_hint: None, None
			size: "20dp", "20dp"
			pos_hint: {"center_x": .73, "center_y": .39}
			on_active:
				root.show_password(*args)

		MDLabel:
			id: pass_text
			text: "Show Password"
			font_size: "10sp"
			pos_hint: {"center_x": .99, "center_y": .39}


		MDRaisedButton:
    		text: "Log In"
    		font_size: '15dp'
    		text_color: 0/255, 0/255, 0/255
    		padding: [90, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .5, 'center_y': .26}
    		md_bg_color: 235/255, 233/255, 187/255, 1
    		on_release:
    			root.loginverification(Lusername.text, Lpassword.text)

    	MDRaisedButton:
    		text: "Create New Account"
    		font_size: '15dp'
    		text_color: 0/255, 0/255, 0/255
    		padding: [39.3, 3]
    		font_name: "LatoB"
    		pos_hint: {'center_x': .5, 'center_y': .17}
    		md_bg_color: 255/255, 222/255, 89/255, 1
    		on_release:
    			root.manager.transition.direction = "left"
    			root.manager.current = "Register_Screen"

