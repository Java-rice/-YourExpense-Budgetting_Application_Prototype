		MdLabel:
			text: "CHOOSE YOUR \nCURRENCY"
			pos_hint: {'center_x': 0.5, 'center_y': 0.7}
			halign: 'center'
			color: 0, 0, 0
			font_size: "10sp"
			font_name: "Montserrat"
			bold: True

		MDDropDownItem
			pos_hint: {'center_x': 0.5, 'center_y': 0.5 }
			text: 'Select Currency'
			font_size: 20
